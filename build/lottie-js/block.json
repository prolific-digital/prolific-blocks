  "icon": "heart",
  "description": "Animate with Lottie.",
  "example": {},
  "supports": {
    "ariaLabel": true,
    "className": true,
    "customClassName": true,
    "html": false
  },
  "attributes": {
    "blockId": {
      "type": "string",
      "default": ""
    },
    "startOnView": {
      "type": "boolean",
      "default": false
    },
    "lottieFile": {
      "type": "string",
      "default": "https://lottie.host/4db68bbd-31f6-4cd8-84eb-189de081159a/IGmMCqhzpt.lottie"
    },
    "loop": {
      "type": "boolean",
      "default": "false"
    },
    "autoplay": {
      "type": "boolean",
      "default": "false"
    },
    "speed": {
      "type": "number",
      "default": 1
    }
  },
  "textdomain": "prolific-blocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js",
  "render": "file:./render.php"
}
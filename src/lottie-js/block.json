{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "prolific/lottie-js",
  "version": "1.0.0",
  "title": "Lottie",
  "category": "prolific",
  "icon": "block-default",
  "description": "Animate with Lottie.",
  "example": {},
  "supports": {
    "align": true,
    "ariaLabel": true,
    "className": true,
    "customClassName": true,
    "html": false
  },
  "attributes": {
    "blockId": {
      "type": "string",
      "default": ""
    },
    "lottieFile": {
      "type": "string",
      "default": "https://lottie.host/4db68bbd-31f6-4cd8-84eb-189de081159a/IGmMCqhzpt.lottie"
    },
    "autoplay": {
      "type": "boolean",
      "default": false
    },
    "loop": {
      "type": "boolean",
      "default": false
    },
    "speed": {
      "type": "number",
      "default": 1
    },
    "direction": {
      "type": "number",
      "default": 1
    },
    "mode": {
      "type": "string",
      "default": "normal"
    },
    "startOnView": {
      "type": "boolean",
      "default": false
    },
    "intermission": {
      "type": "number",
      "default": 0
    },
    "width": {
      "type": "string",
      "default": "400px"
    },
    "height": {
      "type": "string",
      "default": "400px"
    },
    "backgroundColor": {
      "type": "string",
      "default": ""
    },
    "objectFit": {
      "type": "string",
      "default": "contain"
    },
    "useFrameInterpolation": {
      "type": "boolean",
      "default": true
    },
    "segment": {
      "type": "array",
      "default": []
    },
    "marker": {
      "type": "string",
      "default": ""
    }
  },
  "textdomain": "prolific-blocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js",
  "render": "file:./render.php"
}

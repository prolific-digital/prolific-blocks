{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "prolific/breadcrumbs",
  "version": "1.0.0",
  "title": "Breadcrumbs",
  "category": "prolific",
  "icon": "arrow-right-alt2",
  "description": "Display a breadcrumb navigation trail showing the current page location.",
  "example": {},
  "supports": {
    "align": ["wide", "full"],
    "ariaLabel": true,
    "className": true,
    "customClassName": true,
    "html": false,
    "color": {
      "text": true,
      "background": true,
      "link": true
    },
    "typography": {
      "fontSize": true,
      "lineHeight": true
    },
    "spacing": {
      "padding": true,
      "margin": true
    }
  },
  "attributes": {
    "blockId": {
      "type": "string",
      "default": ""
    },
    "divider": {
      "type": "string",
      "default": "slash"
    },
    "showHome": {
      "type": "boolean",
      "default": true
    },
    "homeLabel": {
      "type": "string",
      "default": "Home"
    },
    "showCurrent": {
      "type": "boolean",
      "default": true
    }
  },
  "textdomain": "prolific-blocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}

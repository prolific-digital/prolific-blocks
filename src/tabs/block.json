{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "prolific/tabs",
  "version": "1.0.0",
  "title": "Tabs",
  "category": "prolific",
  "icon": "block-default",
  "description": "A block that allows you to create a tabbed content.",
  "example": {},
  "supports": {
    "ariaLabel": true,
    "className": true,
    "customClassName": true,
    "html": false
  },
  "attributes": {
    "blockId": {
      "type": "string",
      "default": ""
    },
    "tabs": {
      "type": "array",
      "default": [
        {
          "label": "Tab 1"
        },
        {
          "label": "Tab 2"
        }
      ]
    }
  },
  "textdomain": "prolific-blocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js",
  "render": "file:./render.php"
}

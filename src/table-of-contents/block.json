{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "prolific/table-of-contents",
  "version": "1.0.0",
  "title": "Table of Contents",
  "category": "prolific",
  "icon": "list-view",
  "description": "Automatically generate a table of contents from page headings",
  "keywords": ["toc", "contents", "headings", "navigation", "index"],
  "example": {},
  "supports": {
    "align": false,
    "ariaLabel": true,
    "className": true,
    "customClassName": true,
    "html": false,
    "color": {
      "text": true,
      "background": true,
      "link": true
    },
    "typography": {
      "fontSize": true,
      "lineHeight": true
    },
    "spacing": {
      "padding": true,
      "margin": true
    },
    "border": {
      "color": true,
      "radius": true,
      "width": true
    }
  },
  "attributes": {
    "blockId": {
      "type": "string",
      "default": ""
    },
    "title": {
      "type": "string",
      "default": "Table of Contents"
    },
    "showTitle": {
      "type": "boolean",
      "default": true
    },
    "includeH1": {
      "type": "boolean",
      "default": false
    },
    "includeH2": {
      "type": "boolean",
      "default": true
    },
    "includeH3": {
      "type": "boolean",
      "default": true
    },
    "includeH4": {
      "type": "boolean",
      "default": false
    },
    "includeH5": {
      "type": "boolean",
      "default": false
    },
    "includeH6": {
      "type": "boolean",
      "default": false
    },
    "numbered": {
      "type": "boolean",
      "default": false
    },
    "collapsible": {
      "type": "boolean",
      "default": false
    },
    "smoothScroll": {
      "type": "boolean",
      "default": true
    },
    "scrollOffset": {
      "type": "number",
      "default": 100
    }
  },
  "textdomain": "prolific-blocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js",
  "render": "file:./render.php"
}
